.lst_slide_wrap .lst_taps .lst_box .box .lst_slide .swiper-wrapper{}
.lst_slide_wrap .lst_taps .lst_box .box .lst_slide .swiper-slide{padding:0 10px;}
.lst_slide_wrap .lst_taps .lst_box .box .lst_slide .item{}
