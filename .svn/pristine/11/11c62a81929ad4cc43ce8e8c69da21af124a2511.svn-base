/* 개성사전*/
.gsdic_box {padding:120px 0 0 0 }
.gsdic_box .list_top {position:relative;padding:0 10%}

.gsdic_box .title {display:block;font-size: 56px;font-weight: 400;text-align: center;color: #111;line-height: 1.1;letter-spacing: -1px;margin-bottom:50px}
.gsdic_box .title strong {display: inline-block;text-align: right}
.gsdic_box .list_body {position:relative;display: flex;flex-wrap: wrap;padding:50px 0 50px 10% ;}
.gsdic_box .list_body .sub_tx { text-align: left;font-size: 24px;line-height: 1.5;margin-bottom:30px;text-align: justify;word-break: keep-all;word-spacing: -1px}
.gsdic_box .list_body:after {content: "";position: absolute;background-color: #fafafa;left:0;top:18%;width: 100%;height:100%;z-index: -1}

.gsdic_box .list_body .left_box {position:relative;width:calc(100% - 680px);}
.gsdic_box .list_body .left_box b {font-weight: 400}
.gsdic_box .list_body .left_box a:hover {text-decoration: underline}
.gsdic_box .list_body .right_box {position:relative;width:640px;margin-left:40px
}/*background: url('../images/mcms29242.jpg') no-repeat;background-size: 100% auto ;background-position: 50% 0 ;*/ /*background-attachment: fixed;*/

.gsdic_box .list_body .right_box img {animation: figure 1s linear ;width: 100%;}
.gsdic_box .list_body .right_box img {position:relative;left:auto;right:auto;top:auto;bottom:auto;width:auto;height:auto;max-width:100%;min-height:100%;object-fit: cover;margin: auto;object-position: center;transition: all .5s ease-out;}

.gsdic_box .list_body .bot_box {margin-top:50px}
.list_body .full-img {position: relative;width: 100%;height: 300px;overflow: hidden;}
.list_body .full-img figure {position: relative;display: block;animation: figure 1s linear ;}
.list_body .full-img figure {

}

.list_body .full-img.active figure {

}

@keyframes figure {
  0% {
    transform: translateY(100px);
    opacity: 0
  }
  100% {
    transform: translateY(0);
    opacity: 1;
    transition: all .6s
  }
}

@media screen and (max-width: 1500px){
	.gsdic_box .list_body {position: initial}
	.gsdic_box .list_body:after {display: none}
  .gsdic_box .title  {font-size: 40px;}
  .gsdic_box .list_body .sub_tx {font-size: 20px;}
  .gsdic_box .list_body {padding: 50px 2% 50px 2%;}
  .gsdic_box .list_body .sub_tx {word-break:break-all;}
  .gsdic_box .list_body .bot_box {margin-top:0}
  .gsdic_box .list_body .left_box {width: 100%}
  .gsdic_box .list_body .right_box {width:100%;height:100%;overflow: hidden;position: absolute;top:0;left:0;z-index: -1;opacity: 0.5;margin:0;z-index: -1}
  .gsdic_box .list_body .right_box img {max-height: 100%;min-height: 100%;}
  .gsdic_box .list_body .right_box:before {content:"";position: absolute;left:0;top:0;width:100%;height:100%;background-image: linear-gradient(to bottom, rgba(255, 255, 255, 0) 64%, #fff 100%), linear-gradient(to bottom, rgba(255, 255, 255, 0) 0%, #fff 100%);z-index: 1}
  .gsdic_box .list_body .right_box:after {content:"";position: absolute;left:0;top:0;width:100%;height:100%;background-image: radial-gradient(rgb(255, 255, 255) ,rgba(255, 255, 255, 0) 64%)}
}

@media screen and (max-width: 992px){
  .gsdic_box .list_body .left_box a {display: block;text-align: center;background-color: lightgoldenrodyellow;margin:5px 0}
  .gsdic_box .title  {font-size: 30px;}
  .gsdic_box .list_body .sub_tx {font-size: 16px;}
  .gsdic_box .title {margin-bottom:0}

