
*{font-family:'Noto Sans','Noto Sans KR', 'Noto Sans CJK KR', 'Open Sans', '맑은고딕', '나눔고딕', sans-serif}

/* 공통 */
#wrapper{position:relative;width:100%;height:auto;  min-height:100%;  overflow: hidden;}
.width_1800 {width:1800px;max-width: 100%;margin:0 auto;position: relative;}

/* .main_nav.fixedTop .main_nav_box {width:1600px;position: relative;margin:0 auto}
.main_nav.fixedTop .main_nav_box h1 {margin-left:0} */
.main_nav .main_nav_box h1 img {cursor: pointer;}

.nav_inner.flex {height:100px;background-color: #fff;padding:0 0 0 0;display: flex;align-items: center;justify-content: space-between}

.nav_inner.fixedTop {position:fixed;height:100px;border-bottom:1px solid #e4e4e4;width:100%;margin-top:0px;transition: 0.3s ease-in;z-index: 99999;padding:0 60px !important}


.header_all {}
.nav_inner {position: relative;height:100px;background-color: #fff;padding:0 0 0 0;display: flex;align-items: center;justify-content: space-between} 
.header_all .nav_inner .logo_area {width:247px;}
.header_all .nav_inner a {display: block;width: 100%;}
.header_all .nav_inner a img {object-fit: cover;width: auto}
.header_all .nav_inner .nav_area {display: flex;/*margin:0 0 0 180px;*/}
.header_all .nav_inner .nav_area li {text-align: center;padding:0 58px;}
.header_all .nav_inner .nav_area li a {display: block;font-size:22px;color:#000;position: relative;transition: 0.3s ease-in;}
.header_all .nav_inner .nav_area li a::before {transition: 0.3s ease-in;content: "";position: absolute;width:7px;height:7px;background-color: #ffb00d;border-radius: 50%;top:-10px;left:50%;transform: translateX(-50%);opacity: 0;}
.header_all .nav_inner .nav_area li a:hover::before {opacity: 1;}
.header_all .nav_inner .nav_area li a.active::before  {opacity: 1;transition: 0.3s ease-in;content: "";position: absolute;width:7px;height:7px;background-color: #ffb00d;border-radius: 50%;top:-10px;left:50%;transform: translateX(-50%);}

.header_all .nav_inner .search_area {width: 100px;height:100%;}
.header_all .nav_inner .search_area button {width: inherit;height:100%;background-color: #091c54;}
.header_all .nav_inner .search_area button img {width:auto}



  /*관련사이트*/
  .site-all {width: 100%;height:127px;overflow: hidden;border-top:1px solid #ededed}
  .site-all .width_1800 {height:100%;display: flex;align-items: center}
  .site-all ul {width: 80%;margin:0 auto;display: flex;}
  .site-all li {}
  .site-all li a {overflow: hidden;display:flex;align-items: center;padding:5px 5px;height:60px;box-sizing: border-box;border:1px solid #ccc;background-color: #f1f1f1;margin: 5px;}
  .site-all ul .slick-list {overflow: hidden;width: 100%;}
  .site-all ul .slick-prev {background: #000 url(../images/prev_btn.png) no-repeat;background-size: 30%; background-position: center center; border-radius: 50%;}
  .site-all ul .slick-next {background: #000 url(../images/next_btn.png) no-repeat;background-size: 30%; background-position: center center;border-radius: 50%;}
  .site-all li a img {width: 100%;object-position:center;display: inline-block;height:auto;max-height: 50px;min-width: auto;}
  .site-all .slick-prev:before {content: "" !important;}
  .site-all .slick-next:before {content: "" !important;}

  .site-all .slick-dots li.slick-active button:before {display: none;}

.footer_all {width:100%;height:100%;overflow: hidden;}



.footer_all .footer_inner {width:100%;background-color: #050a1b/*#f6f8ff*/;border-top:1px solid #e1e4ef}

.footer_all .footer_inner ul {width: 100%;margin-bottom:0px;margin-top:50px}
.footer_all .footer_inner ul li {margin-right:7px}
.footer_all .footer_inner ul li a {display: block;padding:20px 50px;font-size:16px;color: #fff;font-weight: 300;background-color: #1f2438;}
.footer_all .footer_inner .address_box {overflow:hidden;width:100% ;padding:20px 0 10px 0;/*display:flex;flex-wrap: wrap*/}
.footer_all .footer_inner .address_box dl.rest {color:#8b8e98;font-weight: 300;position: relative;padding-right:50px;float:left}
.footer_all .footer_inner .address_box dl:after {content: "";position: absolute;right:25px;top:50%;width:5px;transform: translateY(-50%);height:5px;background-color: #1f2438;}
.footer_all .footer_inner .address_box dl:last-child:after {width: 0;}
.footer_all .footer_inner .address_box dl:nth-child(2):after {width: 0;}
.footer_all .footer_inner .address_box dl.rest dt {float: left;font-size:16px;margin-right:17px;}
.footer_all .footer_inner .address_box dl.rest dd {display: inline-block}
.footer_all .footer_inner .address_box dl:nth-child(3) {clear: both}

.footer_all .footer_inner .logo_box {position: absolute; right:0;top:50%;/*width:200px;*/width:360px;transform: translateY(-50%);}
.footer_all .footer_inner .logo_box img {opacity: 0.9;}
.footer_all .footer_inner .copy_box {clear: both;width:100%;padding:0px 0px 50px 0;/*background-color: #050a1b;*/opacity: 0.5}
.footer_all .footer_inner .copy_box p {font-size:14px;color: #8b8e98;font-weight: 300;text-align: left;letter-spacing: 1px}



