.mo_visual_main {position:relative;z-index: 100; width:100%;height:calc(100vh - 64px);background-color: #00749f;margin-top:64px}
.mo_visual_main .visual_inner {width: inherit;height:inherit;}
.mo_visual_main .visual_inner ul {width: 100%;height:100%;position: relative;}
.mo_visual_main .visual_inner ul .slick-list {width: 100%;height:100%;}
.mo_visual_main .visual_inner ul .slick-track {width: 100%;height:100%;}
.mo_visual_main .visual_inner ul .slick-slide {width: 100%;height:100%;}
.mo_visual_main .visual_inner ul .slick-slide div {width: 100%;height:100%;}
/* .mo_visual_main .visual_inner ul::after {content:"";position:absolute;top:0;left:0;background-color: rgb(0 0 0 / 30%);width: inherit;height:inherit;z-index: 1;} */
.mo_visual_main .visual_inner ul li {width: 100%;height:100%;}
.mo_visual_main .visual_inner ul li a {position: relative;display: block;width: 100%;height:100%;}
.mo_visual_main .visual_inner ul li a::after {content:"";position:absolute;top:0;right:20px;background: url(../images/mo_main_bg.png) no-repeat;width:112px ;height:238px;z-index: 4;}
.mo_visual_main .visual_inner ul li a .img_bx::after {content:"";position:absolute;top:0;left:0;background-color: rgb(0 0 0 / 30%);width: inherit;height:inherit;z-index: 1;}

.mo_visual_main .visual_inner ul li a .img_bx img {position:relative;top:0px;left:0;width:100%;max-width:100%;height:100%;max-height:100%;margin:auto;vertical-align:middle;object-fit:cover;object-position:center;transition:all .5s ease-out}


.mo_visual_main .visual_inner ul li a .img_bx {position: absolute;top:0;left:0;z-index: 2;width: 100%;height:100%;}

.mo_visual_main .visual_inner ul li a .titie_bx {width:80%;height:auto;  position: absolute;top:30%;left:50%;z-index: 3;transform: translate(-50%, -30%);}

.mo_visual_main .visual_inner ul li a .titie_bx p {font-size: 46px;color:#fff;text-shadow: 2px 0px 7px rgb(0 0 0 / 62%);font-weight: 600;margin-bottom:46px}
.mo_visual_main .visual_inner ul li a .titie_bx span {display:block;font-size: 18px;color:#fff;text-shadow: 2px 0px 7px rgb(0 0 0 / 62%);line-height: 1.5;text-align: justify;}

.mo_visual_main .visual_inner .slick-prev {top:94%;left:20px;z-index: 5;background: url(../images/mo_btn_prev.png);width:37px;height:15px}
.mo_visual_main .visual_inner .slick-next {top:94%;right:20px;z-index: 5;background: url(../images/mo_btn_next.png);width:38px;height:15px}
.mo_visual_main .visual_inner .slick-prev:before {content: "";}
.mo_visual_main .visual_inner .slick-next:before {content: "";}

@media screen and (max-width:1366px){
    .mo_visual_main {display: block !important;}
    .header_all .nav_inner {display: none !important}
    .header_all .visual_main {display: none !important}
}